{% set members = craft.users.group('members').all() %}

<div class="box">
    <div class="box__heading">
        <input type="text" placeholder="Search">
    </div>
    <table>
        <thead>
            <tr>
                <th>Voornaam</th>
                <th>Famillienaam</th>
                <th>Lidnummer</th>
                <th>Lidmaatschap</th>
            </tr>
        </thead>
        <tbody>
            {% for member in members %}
            <tr>
                <td>{{member.altFirstName}}</td>
                <td>{{member.altLastName}}</td>
            </tr>
            {% endfor %}
        </tbody>
    </table>
</div>